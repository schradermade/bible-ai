generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SavedAiResponse {
  id        String   @id @default(cuid())
  userId    String
  feature   String
  reference String?
  prompt    String?
  response  String
  createdAt DateTime @default(now())
}
